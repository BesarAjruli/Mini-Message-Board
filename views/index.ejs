<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Index</title>
</head>
<body>
    <% messages.forEach(message => {%>
        <div>
            <h2><%= message.user %></h2>
            <p><%= message.text %></p>
            <span><%= message.added %></span>
        </div>
    <% })  %>  
</body>
</html>